<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   namespace="ACAPackagesListener.API.Models.Entities"
                   assembly="ACAPackagesListener.API">
  <class name="Device" table="Devices" lazy="false">
    <id name="DeviceId">
      <column name="DeviceId" />
      <generator class="native" />
    </id>
    <property name="Ip"></property>
    <property name="LatLng"></property>
    <property name="Description"></property>
    <property name="Activo"></property>
  </class>
</hibernate-mapping>